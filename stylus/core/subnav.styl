subnav-margin-horizontal = 10px
subnav-margin-vertical = subnav-margin-horizontal
subnav-color = rgb(68, 68, 68, 1)
subnav-hover-color = rgb(0, 119, 221, 1)
subnav-hover-text-decoration = none
subnav-active-color = rgb(0, 119, 221, 1)
subnav-line-border-height = 10px
subnav-line-border = rgb(221, 221, 221, 1)
subnav-line-border-width = 1px
subnav-pill-padding-vertical = 3px
subnav-pill-padding-horizontal = 9px
subnav-pill-hover-background = rgb(238, 238, 238, 1)
subnav-pill-hover-color = rgb(68, 68, 68, 1)
subnav-pill-active-background = rgb(0, 168, 230, 1)
subnav-pill-active-color = rgb(255, 255, 255, 1)
subnav-disabled-color = rgb(153, 153, 153, 1)
/* ========================================================================
   Component: Subnav
 ========================================================================== */
/*
 * 1. Gutter
 * 2. Remove default list style
 */
.uk-subnav
  display -ms-flexbox
  display -webkit-flex
  display flex
  -ms-flex-wrap wrap
  -webkit-flex-wrap wrap
  flex-wrap wrap
  /* 1 */
  margin-left - subnav-margin-horizontal subnav-margin-horizontal
  margin-top - subnav-margin-vertical subnav-margin-vertical
  /* 2 */
  padding 0
  list-style none
/*
 * 1. Space is allocated solely based on content dimensions
 * 2. Horizontal gutter is using `padding` so `uk-width-*` classes can be applied
 * 3. Create position context for dropdowns
 */
.uk-subnav > *
  /* 1 */
  -ms-flex none
  -webkit-flex none
  flex none
  /* 2 */
  padding-left subnav-margin-horizontal
  margin-top subnav-margin-vertical
  /* 3 */
  position relative
/*
 * DEPRECATED IE9 Support
 */
.uk-subnav:before, .uk-subnav:after
  content ""
  display block
  overflow hidden
.uk-subnav:after
  clear both
.uk-subnav > *
  float left
/* Items
 ========================================================================== */
.uk-subnav > * > *
  display inline-block
  color subnav-color
  @extend .hook-subnav
/*
 * Hover
 * 1. Apply hover style also to focus state
 */
.uk-subnav > * > :hover, .uk-subnav > * > :focus
  color subnav-hover-color
  text-decoration subnav-hover-text-decoration
  @extend .hook-subnav-hover
/*
 * Active
 */
.uk-subnav > .uk-active > *
  color subnav-active-color
  @extend .hook-subnav-active
/* Modifier: 'subnav-line'
 ========================================================================== */
.uk-subnav-line > :before
  content ""
  display inline-block
  height subnav-line-border-height
  vertical-align middle
.uk-subnav-line > :nth-child(n + 2):before
  margin-right subnav-margin-horizontal
  border-left subnav-line-border-width solid subnav-line-border
  @extend .hook-subnav-line-divider
/* Modifier: 'subnav-pill'
 ========================================================================== */
.uk-subnav-pill > * > *
  padding subnav-pill-padding-vertical subnav-pill-padding-horizontal
  @extend .hook-subnav-pill
/*
 * Hover
 * 1. Apply hover style also to focus state
 * 2. Remove default focus style
 */
.uk-subnav-pill > * > :hover, .uk-subnav-pill > * > :focus
  background subnav-pill-hover-background
  color subnav-pill-hover-color
  text-decoration none
  /* 2 */
  outline none
  @extend .hook-subnav-pill-hover
/*
 * Active
 * `li` needed for higher specificity to override hover
 */
.uk-subnav-pill > .uk-active > *
  background subnav-pill-active-background
  color subnav-pill-active-color
  @extend .hook-subnav-pill-active
/* Disabled state
 ========================================================================== */
.uk-subnav > .uk-disabled > *
  background none
  color subnav-disabled-color
  text-decoration none
  cursor text
  @extend .hook-subnav-disabled
